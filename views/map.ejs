<div class="map-container">
  <div id="map" class="map"></div>
  <script src="<%-link%> "></script>
  <style>
    .map-container,
    #map {
      margin: 0;
      padding: 0;
      width: 100%;
      height: 100%;
    }
  </style>
</div>
<script>
  
    var map = new MapmyIndia.Map("map", {
    center: [28.61, 77.23],
    zoomControl: true,
    hybrid: true,
    search: true,
    location: true,
  });

  <%-gyms%>.forEach(gym => {
    console.log(gym.location.split(','))
    var marker = L.marker(gym.split(',')).addTo(map).openPopup()
  });
</script>
